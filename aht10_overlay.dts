/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2711";

	fragment@1 {
		target = <&i2c1>;
		__overlay__ {
			status = "okay";
			
            sens1@12 {
                compatible = "mz,sensor";
                reg = <0x18>;
            };
           	aht10@56 {
				compatible = "mz,aht10mz";
				reg = <0x38>;
			};
			aht10@57 {
				compatible = "mz,aht10mz";
				reg = <0x39>;
			};
		};
	};
};
